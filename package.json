{
  "name": "eigo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "run-s deploy:absorb deploy:tts deploy:translate deploy:calibrate deploy:main --print-label",
    "deploy:main": "$npm_execpath run build && wrangler pages deploy dist",
    "deploy:absorb": "wrangler deploy -c wrangler.absorb.toml",
    "deploy:tts": "wrangler deploy -c wrangler.sb-tts.toml",
    "deploy:translate": "wrangler deploy -c wrangler.sb-translate.toml",
    "deploy:calibrate": "wrangler deploy -c wrangler.sb-calibrate.toml",
    "create:migration": "drizzle-kit generate:sqlite --out migrations --schema src/schema.ts",
    "migrate": "wrangler d1 migrations apply eigo-d1 --local",
    "migrate:prod": "wrangler d1 migrations apply eigo-d1",
    "seed": "wrangler d1 execute eigo-d1 --file d1/seed.sql --local",
    "seed:prod": "wrangler d1 execute eigo-d1 --file d1/seed.sql",
    "db:reset": "rm -rf .wrangler/state/v3/d1 && yarn migrate && yarn seed",
    "dev": "run-p dev:main dev:*:p --print-label",
    "dev:main": "vite",
    "dev:absorb": "wrangler dev -c wrangler.absorb.toml --test-scheduled --port 8049",
    "dev:absorb:p": "$npm_execpath run delay 0 && $npm_execpath run dev:absorb",
    "dev:tts": "wrangler dev -c wrangler.sb-tts.toml --port 8050",
    "dev:tts:p": "$npm_execpath run delay 1 && $npm_execpath run dev:tts",
    "dev:translate": "wrangler dev -c wrangler.sb-translate.toml --port 8051",
    "dev:translate:p": "$npm_execpath run delay 2 && $npm_execpath run dev:translate",
    "dev:calibrate": "wrangler dev -c wrangler.sb-calibrate.toml --port 8052",
    "dev:calibrate:p": "$npm_execpath run delay 3 && $npm_execpath run dev:calibrate",
    "build": "vite build --mode client && vite build",
    "preview": "wrangler pages dev dist"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240314.0",
    "@hono/vite-cloudflare-pages": "^0.2.5",
    "@hono/vite-dev-server": "^0.9.0",
    "@types/hls.js": "^1.0.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.18",
    "delay-cli": "^2.0.0",
    "drizzle-kit": "^0.20.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "tailwindcss": "^3.4.1",
    "tailwindcss-safe-area": "^0.5.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-replace": "^0.1.1",
    "vite-plugin-run": "^0.5.1",
    "wrangler": "^3.34.2"
  },
  "dependencies": {
    "@rehooks/local-storage": "^2.4.5",
    "cheerio": "^1.0.0-rc.12",
    "cloudflare-workers-and-google-oauth": "^2.0.0",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.30.2",
    "fast-xml-parser": "^4.3.6",
    "hls.js": "^1.5.7",
    "hono": "^4.1.1",
    "music-metadata-browser": "^2.5.10",
    "openai": "^4.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-intersection-observer": "^9.8.1",
    "react-router-dom": "^6.22.3",
    "react-use-audio-player": "^2.2.0",
    "swr": "^2.2.5",
    "wasm-image-optimization": "^1.0.1"
  }
}
