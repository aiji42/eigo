{
  "name": "eigo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy:index": "wrangler deploy -c wrangler.toml",
    "deploy:absorb": "wrangler deploy -c wrangler.absorb.toml",
    "deploy:tts": "wrangler deploy -c wrangler.sb-tts.toml",
    "create:migration": "drizzle-kit generate:sqlite --out migrations --schema src/schema.ts",
    "migrate": "wrangler d1 migrations apply eigo-d1 --local",
    "migrate:prod": "wrangler d1 migrations apply eigo-d1",
    "seed": "wrangler d1 execute eigo-d1 --file d1/seed.sql --local",
    "seed:prod": "wrangler d1 execute eigo-d1 --file d1/seed.sql",
    "dev": "vite",
    "dev:css": "tailwind -i ./src/index.css -o ./public/static/style.css --watch",
    "dev:absorb": "wrangler dev -c wrangler.absorb.toml --test-scheduled",
    "dev:tts": "wrangler dev -c wrangler.sb-tts.toml --port 8050",
    "build": "vite build --mode client && vite build",
    "preview": "wrangler pages dev dist",
    "deploy": "$npm_execpath run build && wrangler pages deploy dist"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240222.0",
    "@hono/vite-cloudflare-pages": "^0.2.5",
    "@hono/vite-dev-server": "^0.8.1",
    "@types/hls.js": "^1.0.0",
    "@types/react": "^18.2.60",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.17",
    "drizzle-kit": "^0.20.14",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.0.4",
    "vite": "^5.1.4",
    "vite-plugin-replace": "^0.1.1",
		"vite-plugin-run": "^0.5.1",
    "wrangler": "^3.0.0"
  },
  "dependencies": {
    "@cloudflare/ai": "^1.0.53",
    "cheerio": "^1.0.0-rc.12",
    "cloudflare-workers-and-google-oauth": "^2.0.0",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.29.4",
    "fast-xml-parser": "^4.3.5",
    "hls.js": "^1.5.7",
    "hono": "^4.0.7",
    "music-metadata-browser": "^2.5.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.2",
    "react-use-audio-player": "^2.2.0",
    "swr": "^2.2.5"
  }
}
