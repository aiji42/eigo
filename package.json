{
  "name": "eigo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "run-s deploy:task deploy:main --print-label",
    "deploy:main": "$npm_execpath run build && wrangler pages deploy",
    "deploy:task": "wrangler deploy -c wrangler.task.toml",
    "migrate": "wrangler d1 migrations apply eigo-d1 --local",
    "migrate:prod": "wrangler d1 migrations apply eigo-d1",
    "seed": "wrangler d1 execute eigo-d1 --file d1/seed.sql --local",
    "seed:prod": "wrangler d1 execute eigo-d1 --file d1/seed.sql",
    "db:reset": "rm -rf .wrangler/state/v3/d1 && yarn migrate && yarn seed",
    "dev": "run-p dev:*:p --print-label",
    "dev:main": "vite",
    "dev:main:p": "$npm_execpath run delay 3 && $npm_execpath run dev:main",
    "dev:task": "wrangler dev -c wrangler.task.toml --test-scheduled",
    "dev:task:p": "$npm_execpath run dev:task",
    "build": "vite build --mode client && vite build",
    "preview": "wrangler pages dev dist"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240419.0",
    "@hono/vite-cloudflare-pages": "^0.2.5",
    "@hono/vite-dev-server": "^0.11.1",
    "@types/hls.js": "^1.0.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.18",
    "delay-cli": "^2.0.0",
    "drizzle-kit": "^0.20.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "prisma": "^5.12.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-safe-area": "^0.5.1",
    "typescript": "^5.4.5",
    "vite": "^5.1.6",
    "vite-plugin-replace": "^0.1.1",
    "vite-plugin-run": "^0.5.1",
    "wrangler": "^3.51.2"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.10",
    "@hono/zod-validator": "^0.2.1",
    "@mozilla/readability": "^0.5.0",
    "@rehooks/local-storage": "^2.4.5",
    "cloudflare-workers-and-google-oauth": "^2.0.0",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.30.2",
    "fast-xml-parser": "^4.3.6",
    "hls.js": "^1.5.7",
    "hono": "^4.2.6",
    "kiribi": "^0.0.8",
    "linkedom": "^0.16.11",
    "music-metadata-browser": "^2.5.10",
    "openai": "^4.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-intersection-observer": "^9.8.1",
    "react-router-dom": "^6.22.3",
    "swr": "^2.2.5",
    "wasm-image-optimization": "^1.0.1"
  }
}
